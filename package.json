{
  "name": "stark",
  "version": "0.0.0-development",
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "private": true,
  "description": "Stark é uma aplicação para explorar conexões humanas e entender compatibilidade entre indivíduos. Inspirada na força e união da Casa Stark (Game of Thrones), permite que usuários compreendam características pessoais, comparem com outras pessoas e fortaleçam relações em diferentes contextos. Winter is coming",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/nextimecode/stark.git"
  },
  "license": "MIT",
  "author": "NeXTIME",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "arya:build": "turbo run build --filter arya",
    "arya:dev": "turbo run dev --filter arya",
    "arya:e2e": "turbo run e2e --filter arya",
    "arya:e2e:chromium": "turbo run e2e:chromium --filter arya",
    "arya:e2e:codegen": "turbo run e2e:codegen --filter arya",
    "arya:e2e:debug": "turbo run e2e:debug --filter arya",
    "arya:e2e:report": "turbo run e2e:report --filter arya",
    "arya:e2e:ui": "turbo run e2e:ui --filter arya",
    "audit": "pnpm audit",
    "bran:build": "turbo run build --filter bran",
    "bran:dev": "turbo run dev --filter bran",
    "bran:e2e": "turbo run e2e --filter bran",
    "bran:e2e:watch": "turbo run e2e:chromium --filter bran",
    "bran:start:prod": "turbo run start:prod --filter bran",
    "build": "turbo run build --parallel",
    "clean:all": "turbo run clean && pnpm store prune && rm -rf .next .vercel .turbo node_modules tsconfig.tsbuildinfo pnpm-lock.yaml .playwright playwright-report test-results && pnpm install --force && pnpm build",
    "commit": "cz",
    "coverage": "turbo run coverage",
    "depcheck": "depcheck",
    "dev": "prisma generate && turbo run dev --parallel",
    "e2e": "turbo run e2e",
    "postinstall": "if [ \"$CI\" != \"true\" ]; then husky; fi && prisma generate",
    "lint": "turbo run lint --parallel",
    "ned:build": "pnpm prisma && turbo run build --filter ned",
    "ned:dev": "turbo run dev --filter ned",
    "ned:e2e": "turbo run e2e --filter ned",
    "ned:e2e:chromium": "turbo run e2e:chromium --filter ned",
    "ned:e2e:codegen": "turbo run e2e:codegen --filter ned",
    "ned:e2e:debug": "turbo run e2e:debug --filter ned",
    "ned:e2e:report": "turbo run e2e:report --filter ned",
    "ned:e2e:ui": "turbo run e2e:ui --filter ned",
    "outdated": "pnpm outdated",
    "prepare": "husky",
    "release": "semantic-release",
    "release:ui": "turbo run build --filter=ui && changeset publish",
    "reset:all": "pnpm clean:all && pnpm i && turbo run build --parallel",
    "sansa:build": "turbo run build --filter sansa",
    "sansa:dev": "turbo run dev --filter sansa",
    "sansa:e2e": "turbo run e2e --filter sansa",
    "sansa:e2e:chromium": "turbo run e2e:chromium --filter sansa",
    "sansa:e2e:codegen": "turbo run e2e:codegen --filter sansa",
    "sansa:e2e:debug": "turbo run e2e:debug --filter sansa",
    "sansa:e2e:report": "turbo run e2e:report --filter sansa",
    "sansa:e2e:ui": "turbo run e2e:ui --filter sansa",
    "semantic-release:ui": "semantic-release --config semantic-release-ui.config.js",
    "start": "turbo run start",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck --parallel",
    "ui": "turbo run build --filter @nextime/ui",
    "ui:dev": "turbo run dev --filter @nextime/ui",
    "update": "pnpm update --latest",
    "vercel-build": "pnpm install && pnpm build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.817.0",
    "@hookform/resolvers": "^5.0.1",
    "@nestjs/common": "^11.1.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.1",
    "@nestjs/swagger": "^11.2.0",
    "@nextime/auth": "workspace:*",
    "@nextime/tracker": "workspace:*",
    "@nextime/ui": "workspace:*",
    "@prisma/client": "^6.8.2",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/ssr": "^0.6.1",
    "@t3-oss/env-nextjs": "^0.13.4",
    "@tailwindcss/forms": "^0.5.10",
    "@vercel/node": "^5.1.16",
    "apexcharts": "^4.7.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "express": "5.1.0",
    "firebase": "^11.8.1",
    "firebase-admin": "^13.4.0",
    "framer-motion": "^12.12.2",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.511.0",
    "multer": "^2.0.0",
    "next": "^15.3.2",
    "openai": "^4.103.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.8.2",
    "punycode": "^2.3.1",
    "react": "^19.1.0",
    "react-apexcharts": "^1.7.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "react-intersection-observer": "^9.16.0",
    "react-world-flags": "^1.6.0",
    "serverless-http": "^3.2.0",
    "swagger-ui-react": "^5.22.0",
    "tailwind-merge": "^3.3.0",
    "tailwind-variants": "^1.0.0",
    "zod": "^3.25.28",
    "zod-openapi": "^4.2.4",
    "zod-validation-error": "^3.4.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.29.4",
    "@faker-js/faker": "^9.8.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/testing": "^11.1.1",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/express": "^5.0.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.21",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/react-world-flags": "^1.6.0",
    "@types/swagger-ui-react": "^5.18.0",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "depcheck": "^1.4.7",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "openapi-types": "^12.1.3",
    "openapi3-ts": "^4.4.0",
    "postcss": "^8.5.3",
    "semantic-release": "^24.2.5",
    "supertest": "^7.1.1",
    "tailwindcss": "4.1.7",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3",
    "unplugin-swc": "^1.5.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "pnpm": {
    "overrides": {
      "undici": "^7.9.0",
      "esbuild": "^0.25.4",
      "prismjs": "^1.30.0"
    }
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
