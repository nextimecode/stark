{
  "name": "stark",
  "version": "0.0.0-development",
  "description": "Stark é uma aplicação para explorar conexões humanas e entender compatibilidade entre indivíduos. Inspirada na força e união da Casa Stark (Game of Thrones), permite que usuários compreendam características pessoais, comparem com outras pessoas e fortaleçam relações em diferentes contextos. Winter is coming",
  "packageManager": "pnpm@9.14.2",
  "engines": {
    "node": "22.11.0"
  },
  "private": true,
  "keywords": [],
  "author": "NeXTIME",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "vercel-build": "pnpm install && pnpm build",
    "build": "turbo run build --parallel",
    "clean:all": "turbo run clean && pnpm store prune && rm -rf .next .turbo node_modules tsconfig.tsbuildinfo pnpm-lock.yaml && pnpm i && pnpm build",
    "coverage": "turbo run coverage",
    "e2e": "turbo run e2e",
    "test": "turbo run test",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint --parallel",
    "audit": "pnpm audit",
    "outdated": "pnpm outdated",
    "release": "semantic-release",
    "semantic-release:ui": "semantic-release --config semantic-release-ui.config.js",
    "release:ui": "turbo run build --filter=ui && changeset publish",
    "reset:all": "pnpm clean:all && pnpm i && turbo run build --parallel",
    "start": "turbo run start",
    "typecheck": "turbo run typecheck --parallel",
    "update": "pnpm update --latest",
    "arya:build": "turbo run build --filter arya",
    "arya:dev": "turbo run dev --filter arya",
    "arya:e2e": "turbo run e2e --filter arya",
    "arya:e2e:ui": "turbo run e2e:ui --filter arya",
    "arya:e2e:chromium": "turbo run e2e:chromium --filter arya",
    "arya:e2e:debug": "turbo run e2e:debug --filter arya",
    "arya:e2e:codegen": "turbo run e2e:codegen --filter arya",
    "arya:e2e:report": "turbo run e2e:report --filter arya",
    "sansa:build": "turbo run build --filter sansa",
    "sansa:dev": "turbo run dev --filter sansa",
    "sansa:e2e": "turbo run e2e --filter sansa",
    "sansa:e2e:ui": "turbo run e2e:ui --filter sansa",
    "sansa:e2e:chromium": "turbo run e2e:chromium --filter sansa",
    "sansa:e2e:debug": "turbo run e2e:debug --filter sansa",
    "sansa:e2e:codegen": "turbo run e2e:codegen --filter sansa",
    "sansa:e2e:report": "turbo run e2e:report --filter sansa",
    "bran:build": "turbo run build --filter bran",
    "bran:dev": "turbo run dev --filter bran",
    "ui:build": "turbo run build --filter ui",
    "ui:dev": "turbo run dev --filter ui",
    "precommit": "turbo lint typecheck audit",
    "commit": "cz",
    "prepare": "husky",
    "postinstall": "husky",
    "depcheck": "depcheck"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@next/font": "^14.2.15",
    "@nextime-ui/preline": "file:/Users/pedroduarte/dev/nextime-ui/packages/preline",
    "@preline/theme-switch": "^2.5.0",
    "@radix-ui/react-collapsible": "1.1.1",
    "@radix-ui/react-scroll-area": "1.2.1",
    "@radix-ui/react-select": "2.1.2",
    "@radix-ui/react-slot": "1.1.0",
    "@radix-ui/react-tabs": "1.1.1",
    "@t3-oss/env-nextjs": "^0.11.1",
    "datatables.net": "^2.1.8",
    "datatables.net-dt": "^2.1.8",
    "dropzone": "6.0.0-beta.2",
    "eslint-plugin-unicorn": "^56.0.1",
    "framer-motion": "11.12.0",
    "lucide-react": "^0.462.0",
    "next": "^15.0.3",
    "preline": "^2.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-world-flags": "1.6.0",
    "tailwind-merge": "^2.5.5",
    "tailwind-variants": "0.3.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@nextime-ui/eslint-config": "^3.1.9",
    "@nextime-ui/ts-config": "^3.1.9",
    "@playwright/test": "^1.49.0",
    "@swc/core": "^1.9.3",
    "@tailwindcss/forms": "^0.5.9",
    "@tsconfig/node22": "^22.0.0",
    "@turbo/gen": "^2.3.3",
    "@types/node": "22.10.1",
    "@types/prop-types": "^15.7.13",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/react-world-flags": "^1.6.0",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.16.0",
    "eslint-config-next": "^15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^2.3.3",
    "eslint-plugin-import-helpers": "^2.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prop-types": "^15.8.1",
    "semantic-release": "^24.2.0",
    "semantic-release-monorepo": "^8.0.2",
    "tailwindcss": "^3.4.15",
    "tsup": "^8.3.5",
    "turbo": "^2.3.3",
    "typescript": "5.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nextimecode/stark.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
